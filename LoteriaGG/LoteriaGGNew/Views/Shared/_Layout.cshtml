                data: $(form).serialize(),
                success: function (data) {
                    if (data.data == "Exito") {
                        location.reload();
                    } else {
                        Materialize.toast(data.data, 3000, 'red');
                    }
                }
            })
        }

        function Register() {
            var form = $('#formRegistro');

            $.ajax({
                type: 'POST',
                url: $(form).attr('action'),
                data: $(form).serialize(),
                success: function (data) {
                    if(data.exito == "true")
                        location.reload();
                    if (data.exito == "false") {
                        Materialize.toast(data.mensaje, 3000, 'red');
                    }
                }
            })
        }

        function Logout() {
            location.href = "@Url.Action("Logout", "Home")"
        }

        function ActualizarGGCoins(cantidad) {
            $('.ggcoins').html(cantidad + ' GGCoins');
        }
    </script>
