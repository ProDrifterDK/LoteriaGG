    <script>
    window.fbAsyncInit = function () {
        FB.init({
            appId: '122229375033808', //id local 787191674781997 //122229375033808
            oauth: true,
            cookie: true,
            xfbml: true,
            version: 'v2.8'
        });
        FB.AppEvents.logPageView();
    };
    (function (d, s, id) {
        var js, fjs = d.getElementsByTagName(s)[0];
        if (d.getElementById(id)) { return; }
        js = d.createElement(s); js.id = id;
        js.src = "//connect.facebook.net/en_US/sdk.js";
        fjs.parentNode.insertBefore(js, fjs);
    }(document, 'script', 'facebook-jssdk'));

    function myFacebookLogin() {
        FB.login(function (response) {
            if (response.authResponse) {
                checkLoginState();
            }
        }, { scope: 'publish_actions,email,user_mobile_phone', perms:'user_mobile_phone' },);
    }

    function checkLoginState() {
        FB.getLoginStatus(function (response) {
            statusChangeCallback(response);
            console.log(response.authResponse.accessToken);
        });
        FB.api('/me?fields=id,name,email,permissions', function (response) {
            var url = "@Url.Action("FBLogin")";
            var mail = response.email == undefined ? response.id : response.email;
            location.href = url + "?mail=" + mail + "&usr=" + response.name;
        });
    }
    </script>
